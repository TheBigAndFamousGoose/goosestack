{
  "auth": {
    "profiles": {
      {{GOOSE_AUTH_PROFILE}}
    }
  },
  "agents": {
    "defaults": {
      "workspace": "~/.openclaw/workspace",
      "memorySearch": {
        "provider": "local",
        "fallback": "none"
      },
      "compaction": {
        "mode": "safeguard"
      },
      "thinkingDefault": "low",
      "elevatedDefault": "on",
      "timeoutSeconds": 600,
      "maxConcurrent": 4,
      "subagents": {
        "maxConcurrent": 8,
        "model": "ollama/{{GOOSE_OLLAMA_MODEL}}",
        "thinking": "off"
      }
    }
  },
  "tools": {
    "web": {
      "search": {
        "provider": "brave"
      }
    }
  },
  "commands": {
    "native": "auto",
    "nativeSkills": "auto"
  },
  "channels": {
    {{GOOSE_TELEGRAM_CONFIG}}
  },
  "gateway": {
    "port": 18789,
    "mode": "local",
    "bind": "loopback",
    "auth": {
      "mode": "token",
      "token": "{{GOOSE_GATEWAY_TOKEN}}"
    }
  },
  "plugins": {
    "entries": {
      {{GOOSE_TELEGRAM_PLUGIN}}
    }
  }
}
